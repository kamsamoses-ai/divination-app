{
  "version": 2,
  "builds": [
    {
      "src": "api/divine.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/divine",
      "dest": "/api/divine.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|gif|ico|json|map))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
